{
    "name": "homepage-template",
    "version": "0.0.7",
    "description": "A module that generates a homepage for your projects",
    "repository": {
        "type": "git",
        "url": "git://github.com/thieryw/homepage-template.git"
    },
    "main": "dist/lib/index.js",
    "types": "dist/lib/index.d.ts",
    "scripts": {
        "test": "node dist/test/",
        "build": "tsc && yarn copyfiles",
        "copyfiles": "copyfiles -u 1 src/**/*.svg dist/",
        "lint:check": "eslint . --ext .ts,.tsx",
        "lint": "npm run lint:check -- --fix",
        "_format": "prettier '**/*.{ts,tsx,json,md}'",
        "format": "npm run _format -- --write",
        "format:check": "npm run _format -- --list-different"
    },
    "lint-staged": {
        "*.{ts,tsx}": [
            "eslint --fix"
        ],
        "*.{ts,tsx,json,md}": [
            "prettier --write"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged -v"
        }
    },
    "author": "u/thieryw",
    "license": "MIT",
    "files": [
        "src/",
        "!src/test/",
        "dist/",
        "!dist/test/",
        "!dist/tsconfig.tsbuildinfo"
    ],
    "keywords": [],
    "homepage": "https://github.com/thieryw/homepage-template",
    "peerDependencies": {
        "react": "*"
    },
    "optionalDependencies": {
        "@types/react": "*"
    },
    "dependencies": {
        "@material-ui/core": "^4.11.3",
        "@material-ui/icons": "^4.11.2",
        "embla-carousel": "^4.1.5",
        "powerhooks": "^0.0.37",
        "react-markdown": "^5.0.3",
        "react-svg": "^13.0.4",
        "tss-react": "^0.0.8",
        "typeface-work-sans": "^1.1.13"
    },
    "devDependencies": {
        "@types/node": "^15.0.2",
        "@types/react": "^17.0.4",
        "@typescript-eslint/eslint-plugin": "^4.15.1",
        "@typescript-eslint/parser": "^4.15.1",
        "copyfiles": "^2.4.1",
        "eslint": "^7.20.0",
        "eslint-config-prettier": "^7.2.0",
        "evt": "^2.0.0-beta.18",
        "husky": "^4.3.0",
        "lint-staged": "^10.5.4",
        "prettier": "^2.2.1",
        "react": "^16.14.0",
        "react-svg": "^13.0.4",
        "typescript": "^4.1.5"
    }
}
